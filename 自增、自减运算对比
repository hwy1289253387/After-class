#include<iostream>
using namespace std;
int main()
{int a=10;
++a;
cout<<a<<endl;
return 0;
  }  
  #include<iostream>
using namespace std;
int main()
{int a=10;
int b=++a;
cout<<a<<","<<b<<endl;
return 0;
  }  
