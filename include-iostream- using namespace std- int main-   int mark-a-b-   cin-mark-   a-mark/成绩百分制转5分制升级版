#include<iostream>
using namespace std;
int main(){
  int mark,a,b;
  cin>>mark;
  a=mark/10;
  b=mark%10;
  
  switch(a)
  {
  	case 10:
  	case 9:
  		if(b>=5&&b<=9) cout<<"5";
  		else cout<<"4.5";
  		break;
  	case 8:
	    if(b>=5&&b<=9) cout<<"4";
		else cout<<"3.5";
		break;	
  	case 7:
	  	if(b>=5&&b<=9) cout<<"3";
	  	else cout<<"2.5";
	  	break;
	case 6:
		if(b>=5&&b<=9) cout<<"2";
	  	else cout<<"1.5";
	  	break;
	case 5:
		if(b>=5&&b<=9) cout<<"1";
	  	else cout<<"0.5";
	  	break;
	default:cout<<"0.00";
  }
   
return 0;
}
