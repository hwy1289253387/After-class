#include<iostream>
using namespace std;
void swap(int &x,int &y) 
{int t;
t=x;x=y;y=t;
cout<<"x="<<x<<","<<"y="<<y<<endl;
}
int main(){
int a(4),b(5);//等价于a=4,b=5
cout<<"交换前：" <<endl;
cout<<"a="<<a<<","<<"b="<<b<<endl;
swap(a,b) ;
cout<<"交换后："<<endl;
cout<<"a="<<a<<","<<"b="<<b<<endl;
 
return 0;
}
